{
    "name": "novatorius/config-db-schema",
    "description": "Abstractions to help creating config tables.",
    "type": "package",
    "homepage": "https://github.com/phpnomad/core",
    "readme": "README.md",
    "license": "GPL-3.0-or-later",
    "scripts": {
        "php-cs-fixer": "php-cs-fixer fix"
    },
    "authors": [
        {
            "name": "Alex Standiford",
            "email": "alex@standiford.us"
        }
    ],
    "autoload": {
        "psr-4": {

    "require": {
        "phpnomad/datastore": "^2.0",
        "phpnomad/utils": "^1.0",
        "phpnomad/db": "^2.0",
        "phpnomad/facade": "^1.0",
        "phpnomad/di": "^1.0",
        "phpnomad/loader": "^1.0"
